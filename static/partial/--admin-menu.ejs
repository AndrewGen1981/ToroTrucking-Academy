<style>
    :root {
        --manu-item-col: rgba(255, 255, 255, 0.95);
        --manu-item-col-hover: rgba(255, 255, 255);
    }

    .-admin-menu-btn {
        all: unset;

        margin: 0 5px;
        width: 45px;
        height: 35px;
        
        display: inline-flex;
        align-items: center;
        justify-content: center;

        font-size: 1.15rem;
        font-weight: normal;
        
        text-decoration: none;
        
        color: var(--manu-item-col);
        background: transparent;
        
        border: 2px solid var(--manu-item-col);
        border-radius: 30px;

        cursor: pointer;
    }
    .-admin-menu-btn:hover {
        color: var(--manu-item-col-hover);
        border: 2px solid var(--manu-item-col-hover);

        transform: translateY(-1px);
        transition: all 0.25s linear;
    }

    .-admin-menu-btn svg {
        height: 21px !important;
        fill: var(--manu-item-col);
    }
</style>


