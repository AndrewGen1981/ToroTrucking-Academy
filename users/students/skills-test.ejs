                            <span class="-m-transmission">
                                <select>
                                    <option value="Automatic">Automatic</option>
                                    <option value="Manual">Manual</option>
                                </select>
                            </span>
                            
                            <span class="-m-strf">
                                <select>
                                    <option value="Paid" selected>Paid</option>
                                    <option value="Paid" selected>to Pay</option>
